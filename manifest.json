{
  "manifest_version": 2,
  "name": "Fast forward",
  "options_ui":{
	"page": "options.html"
	},
  "version": "1.0",
  "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs","activeTab","storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Fast forward"
  },
  	"icons": {
        "128": "icon.png"
    }
}
